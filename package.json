{
  "name": "boilerplate-be",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "knex": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts",
    "migrate": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:latest",
    "migrate:rollback": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:rollback",
    "migrate:current": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:current",
    "migrate:make": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:make",
    "seed": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts seed:run",
    "seed:make": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts seed:make",
    "reset-db": "yarn run migrate:rollback --all && yarn run migrate && yarn run seed && yarn start"
  },
